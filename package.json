{
  "name": "server-side",
  "version": "1.0.0",
  "description": "An API Implmentation of the More-Recipes specs",
  "main": "server.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc mocha --exit server/test/test.js ",
    "s:test": "set NODE_ENV=test-l&& nyc mocha --exit --require babel-register  server/test/test.js",
    "start:dev1": "node --require babel-register server/lib/server.js",
    "start:dev": "nodemon --require babel-register server/lib/server.js",
    "coveralls": "npm test && nyc report --reporter=text-lcov _mocha | coveralls",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "shc": "start coverage/lcov-report/index.html",
    "seq": "node_modules/.bin/sequelize --require babel-register init",
    "build": "babel server/lib/ -d server/dist",
    "start": "node server/dist/server.js"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "keywords": [
    "Node",
    "API",
    "Express"
  ],
  "author": "Idrees Ibraheem",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "dotenv": "^4.0.0",
    "eslint": "^4.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.2",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "^8.1.0",
    "mocha": "^4.0.1",
    "morgan": "^1.9.0",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.20.1",
    "sequelize-cli": "^3.0.0",
    "supertest": "^3.0.0"
  },
  "devDependencies": {
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.2.1",
    "uuid": "^3.1.0"
  }
}
