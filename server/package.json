{
  "name": "server-side",
  "version": "1.0.0",
  "description": "An API Implmentation of the More-Recipes specs",
  "main": "server.js",
  "scripts": {
    "start:dev": "node_modules/.bin/babel-node lib/server.js --presets es2015,stage-2",
    "start": "nodemon lib/server.js --exec babel-node",
    "build": "babel lib -d dist",
    "serve": "node dist/server.js",
    "test": "mocha --compilers js:babel-core/register"
